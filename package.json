{
  "name": "deno",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "xcode": "xed -b ios",
    "setup:ios": "cd ios && pod install --repo-update && cd ..",
    "start:sta": "APP_ENV=staging npx react-native start --reset-cache",
    "start:dev": "APP_ENV=development npx react-native start --reset-cache",
    "start:pro": "APP_ENV=production npx react-native start --reset-cache"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.7",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@shopify/restyle": "^2.1.0",
    "@tanstack/react-query": "^4.2.1",
    "axios": "^0.27.2",
    "i18next": "^21.9.1",
    "react": "18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.34.2",
    "react-i18next": "^11.18.4",
    "react-native": "0.69.3",
    "react-native-dotenv": "^3.3.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-flash-message": "^0.3.1",
    "react-native-localize": "^2.2.3",
    "react-native-mmkv": "^2.4.3",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.16.0",
    "react-native-svg": "^13.0.0",
    "yup": "^0.32.11",
    "zustand": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
